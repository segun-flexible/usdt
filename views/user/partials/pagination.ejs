<div class="card-footer">
    <nav aria-label="...">
        <ul class="pagination justify-content-end mb-0">
            
            <%if(prevBtn){%>
            <li class="page-item">
                <a class="page-link" href="<%=link%>page=<%=prevBtn%>" tabindex="-1">
                    <i class="fas fa-angle-left"></i>
                    <span class="sr-only">Previous</span>
                </a>
            </li>
            <%}else{%>
            <li class="page-item disabled">
                <a class="page-link" href="javascript:void(0)" tabindex="-1">
                    <i class="fas fa-angle-left"></i>
                    <span class="sr-only">Previous</span>
                </a>
            </li>
            <%}%>
            <%let count=1; paginationArr.slice(currentPage - 1).map(p=>{%>
            
                <%if(count <=6){%>
            
                    <%if(currentPage===p){%>
                        <li class="page-item active">
                            <a class="page-link" href="javascript:void(0)"><%=p%></a>
                        </li>
            
                        <%}else{%>
                        <li class="page-item"><a class="page-link" href="<%=link%>page=<%=p%>"><%=p%></a></li>
            
                            <%}%>
            
                                <%}%>
            
                                    <%count +=1%>
            
                                        <%})%>

            <%if(nextBtn){%>
            <li class="page-item">
                <a class="page-link" href="<%=link%>page=<%=nextBtn%>" tabindex="-1">
                    <i class="fas fa-angle-right"></i>
                    <span class="sr-only">Next</span>
                </a>
            </li>
            <%}else{%>
            <li class="page-item disabled">
                <a class="page-link" href="javascript:void(0)" tabindex="-1">
                    <i class="fas fa-angle-right"></i>
                    <span class="sr-only">Next</span>
                </a>
            </li>
            <%}%>

            
        </ul>
    </nav>
</div>

